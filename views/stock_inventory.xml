<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="djbc.view_stock_inventory_form" model="ir.ui.view">
		<field name="name">djbc.stock_inventory</field>
		<field name="model">stock.inventory</field>
		<field name="inherit_id" ref="stock.view_inventory_form"/>
		<field name="arch" type="xml">
			<field name="product_ids" position="after">
				<field name="djbc_mark"/>
			</field>
		</field>
	</record>
	<record id="djbc.view_stock_inventory_tree" model="ir.ui.view">
		<field name="name">djbc.stock_inventory</field>
		<field name="model">stock.inventory</field>
		<field name="inherit_id" ref="stock.view_inventory_tree"/>
		<field name="arch" type="xml">
			<field name="date" position="after">
				<field name="djbc_mark"/>
			</field>
		</field>
	</record>

</odoo>
