<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="djbc_categs_tree" model="ir.ui.view">
		<field name="name">DJBC Categories Tree</field>
		<field name="model">djbc.categs</field>
		<field name="arch" type="xml">
			<tree string="DJBC Categories">
				<field name="name"/>
			</tree>
		</field>
	</record> 

	<record id="djbc_categs_action" model="ir.actions.act_window">
		<field name="name">DJBC Categories</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">djbc.categs</field>
		<!--field name="view_type">form</field-->
		<field name="view_mode">tree,form</field>
   		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				It's Empty
			</p>
		</field>
	</record>
</odoo>
